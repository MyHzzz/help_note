function git_exefun(){ 
    #echo $#
    arg1=$2
    fun=$1
    if [ "$#" = "2" ]   #commit help
    then
        if [ "$arg1" = "help" ]   
        then
            $fun $git_help_note
        elif [ "$arg1" = "base" ]
        then
            $fun $git_base
            $fun $git_BaseSSM
        elif [ "$arg1" = "cc" ]
        then
            $fun $git_cc
        fi
    else
        $fun $git_help_note
        $fun $git_base
        $fun $git_BaseSSM
        $fun $git_cc
        $fun $git_GraphicsTools
    fi 
}
function git_add_commit_pull(){
    echo 'update add/commt/pull: '$1
    cd $1
    toolsLineLong >> $git_log
    pwd >> $git_log
    git add . >> $git_log
    git commit -am "`date`" >> $git_log
    git pull origin master >> $git_log
    toolsLineLong >> $git_log 
}
function git_add_commit_push(){
    echo 'upload add/commt/push: '$1
    cd $1
    toolsLineLong >> $git_log
    pwd >> $git_log
    git add . >> $git_log
    git commit -am "`date`" >> $git_log
    git push -u origin master >> $git_log
    toolsLineLong >> $git_log 
}

function git_add_commit(){
    echo 'commit add/commt: '$1
    cd $1
    toolsLineLong >> $git_log
    pwd >> $git_log
    git add . >> $git_log
    git commit -am "`date`" >> $git_log
    toolsLineLong >> $git_log
}

